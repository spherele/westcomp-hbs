<!DOCTYPE html>
<html data-theme="light" class="page page--{{ title }}">
  <head>
    {{> layouts/head }}

    <title>{{ title }}</title>

    <style>
      .gui {
        position: fixed;
        background-color: var(--white);
        padding: 0.5rem 1rem;
        box-shadow: 0 0 10px rgba(var(--black-rgb), 0.25);
        z-index: 10;
        right: 1rem;
        bottom: 1rem;
        border-radius: 4px;
      }

      .sitemap-section .container > div > div:not(:last-child) {
        margin-bottom: 2rem;
      }

      table {
        width: 100%;
        table-layout: fixed;
      }

      table:not(:last-child) {
        margin-bottom: 2rem;
      }

      table td,
      table th {
        padding: 10px;
        vertical-align: top;
      }

      table td {
        border: 1px dashed var(--gray-100);
      }

      table td > div:not(:last-child) {
        margin-bottom: 5px;
      }

      table td > div .button {
        width: 100%;
        margin: 0;
      }

      table td > div .button-icon {
        width: auto;
      }

      table td > .input-wrapper {
        margin: 0;
      }

      table td > .dropdown {
        margin: 0;
      }

      table td > .select {
        margin: 0;
      }

      table td > .select.select--flat {
        width: auto;
      }
    </style>
  </head>


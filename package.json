{
  "name": "boilerplate",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack serve --progress --color --config config/webpack.dev.js",
    "build": "cross-env NODE_ENV=production webpack --progress --color --config config/webpack.prod.js",
    "build:prod": "cross-env NODE_ENV=production FOR=backend webpack --progress --color --config config/webpack.prod.js",
    "lint": "yarn-run-all lint:all",
    "lint:all:eslint": "yarn lint:eslint --ext .js,",
    "lint:all:prettier": "yarn lint:prettier \"**/*.{js,json,css,scss,html,md}\"",
    "lint:all:stylelint": "yarn lint:stylelint \"src/**/*.{scss}\"",
    "lint:eslint": "eslint --fix",
    "lint:prettier": "prettier --write --loglevel warn",
    "lint:stylelint": "stylelint --fix"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.8",
    "@babel/core": "^7.12.9",
    "@babel/eslint-parser": "^7.21.3",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.12.1",
    "@babel/plugin-proposal-optional-chaining": "^7.12.7",
    "@babel/preset-env": "^7.12.7",
    "@webpack-cli/serve": "^1.1.0",
    "address": "^1.1.2",
    "autoprefixer": "^10.0.4",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^3.0.0",
    "critical-css-webpack-plugin": "^3.0.0",
    "cross-env": "^7.0.2",
    "css-loader": "^5.0.1",
    "cssnano": "^4.1.10",
    "eslint": "^7.15.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "file-loader": "^6.2.0",
    "handlebars": "^4.7.8",
    "handlebars-loader": "^1.7.1",
    "html-loader": "^1.3.2",
    "html-webpack-injector": "^1.1.4",
    "html-webpack-plugin": "^4.5.0",
    "husky": "^4.3.5",
    "imagemin-lint-staged": "^0.4.0",
    "imagemin-mozjpeg": "^9.0.0",
    "imagemin-webp-webpack-plugin": "^3.3.3",
    "imagemin-webpack-plugin": "^2.4.2",
    "lint-staged": "^10.5.3",
    "mini-css-extract-plugin": "^1.3.1",
    "postcss": "^8.1.10",
    "postcss-loader": "^4.1.0",
    "preload-webpack-plugin": "3.0.0-beta.3",
    "prettier": "^2.2.1",
    "sass": "^1.29.0",
    "sass-loader": "^10.1.0",
    "style-loader": "^2.0.0",
    "stylelint": "^13.8.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-recess-order": "^2.3.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-scss": "^3.18.0",
    "svg-sprite-loader": "^5.1.1",
    "terser-webpack-plugin": "4.2.3",
    "webpack": "^4.41.5",
    "webpack-cli": "^4.2.0",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^5.4.0",
    "zip-webpack-plugin": "^4.0.1"
  },
  "dependencies": {
    "@floating-ui/dom": "^1.6.5",
    "@glidejs/glide": "^3.6.1",
    "@popperjs/core": "^2.11.7",
    "core-js": "^3.8.0",
    "fs": "^0.0.1-security",
    "inputmask": "^5.0.7",
    "mark.js": "^8.11.1",
    "nouislider": "^15.7.1",
    "overlayscrollbars": "^2.1.0",
    "scroll-lock": "^2.1.4",
    "swiper": "^11.0.5",
    "tippy.js": "^6.3.7"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
